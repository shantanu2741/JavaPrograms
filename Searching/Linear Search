import java.util.*;
public class LinearSearch {
    public static void main(String args[])
    {
       int arr[]={2,222,343,21,342,45,21,32,12,3};
       linearSearch(arr,12); 
    }
    static void linearSearch(int arr[],int target)
    {
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]==target)
            {
                System.out.println(i);
                break;
            }
        }
    }
}
