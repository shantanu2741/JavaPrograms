import java.util.*;
public class MinMax {
    public static void main(String args[])
    {
       int arr[]={2,3,22,44,55,66,33,21};
       minMax(arr);
    }

    static void minMax(int arr[])
    {
        int min=arr[0];
        int max=arr[0];

        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]>max)
            {
                max=arr[i];
            }
        }
        for(int i=0;i<arr.length;i++)
        {
            if(arr[i]<min)
            {
                min=arr[i];
            }
        }
        System.out.println(min);
        System.out.println(max);
    }
}
